<!-- location of html tag element(xpath) in amazon.com site -->
<AMAZON>
  <PAGE>http://amazon.com</PAGE>
  <LOAD-TIMEOUT>7</LOAD-TIMEOUT>
  <!-- GOOGLE CHROME WEB BROWSER -->
  <GOOGLE-PAGE>
    <LANDING>
      <ZIP-CODE-VAL>97124</ZIP-CODE-VAL>
      <ZIP-CODE-BTN>//*[@id="nav-global-location-popover-link"]</ZIP-CODE-BTN>
      <INPUT-BOX>//*[@id="GLUXZipUpdateInput"]</INPUT-BOX>
      <APPLY-BTN>//*[@id="GLUXZipUpdate"]</APPLY-BTN>
      <!-- Please enter a valid US zip code -->
      <ERROR-RETRY>//*[@id="GLUXZipError"]/div/div/div</ERROR-RETRY>
      <DONE-BTN>//*/div[@class="a-popover-wrapper"]/div[@class="a-popover-footer"]/span/span/span/button</DONE-BTN>
      <CONTINUE-BTN>//*/div[@class="a-popover-wrapper"]/div[@class="a-popover-footer"]/span/span</CONTINUE-BTN>
      <WAIT-ZIPCODE-CHANGE>2</WAIT-ZIPCODE-CHANGE>
      <LOC-ADDR>//*[@id="glow-ingress-line2"]</LOC-ADDR>
    </LANDING>
    <PRODUCT>
      <TITLE>//*/span[@id="productTitle"]</TITLE>
      <CASE1>
        <PRICE-BLOCK>//*/span[@id="priceblock_ourprice"]</PRICE-BLOCK>
      </CASE1>
      <CASE2>
        <PRICE-BLOCK>//*/span[@id="priceblock_dealprice"]</PRICE-BLOCK>
      </CASE2>
      <CASE3>
        <PRICE-BLOCK>//*/span[@id="priceblock_saleprice"]</PRICE-BLOCK>
      </CASE3>
      <CASE4>
        <!-- Currently unavailable. -->
        <AVAILABILITY>//*[@id="availability"]/span</AVAILABILITY>
      </CASE4>
      <CASE5>
        <!-- Available from {these sellers.}-->
        <AVAILABILITY>//*/div[@id="availability"]/span/span[2]</AVAILABILITY>
        <!-- //*[@id="olp_feature_div"]/div[2]/span/a/span[2] -->
        <PRICE-BLOCK>//*[@id="olp_feature_div"]/div[@class="a-section a-spacing-small a-spacing-top-small"]/span/a[@class="a-link-normal"]/span[@class="a-size-base a-color-price"]</PRICE-BLOCK>
      </CASE5>
    </PRODUCT>
  </GOOGLE-PAGE>
